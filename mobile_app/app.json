{
  "expo": {
    "name": "Feedback App",
    "slug": "feedback-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mobileapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.himanshu.feedback"
    },

    "web": {
      "output": "static",
      "favicon": "./assets/images/new.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#f8f9ff",
          "image": "./assets/images/new.png",
          "resizeMode": "contain",
          "dark": {
            "backgroundColor": "#f8f9ff",
            "image": "./assets/images/new.png",
            "resizeMode": "contain"
          }
        }
      ],
      "expo-font"
    ],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "55ebfd5c-9a1e-46ea-b436-5e14efd4b09d"
      }
    }
  }
}
